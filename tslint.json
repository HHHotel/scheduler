{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": true,
    "rules": {
        "prefer-for-of": false,
        "no-console": true,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "completed-docs": true,
        "typedef": true
    }
}
